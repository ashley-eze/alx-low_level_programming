#include "main.h"
/**
* _strcpy - copies a string including the null byte to a buffer
* @desb: destination buffer
* @src: source string
*
* Return: pointer to dest
*/
char *_strcpy(char *desb, const char *src)
{
char *ptr = desb;

while (*src)
{
*desb = *src;
desb++;
src++;
}

*desb = '\0';
return ptr;
}
